<!-- file_name : views/model_name_views.xml -->
<odoo>
    <data>
        <!-- inherit existing window action to modify -->
        <record id="module_name.window_action_external_id" model="ir.actions.act_window">
            <field name="field_name">New_Value</field>
        </record>
    </data>
</odoo>

